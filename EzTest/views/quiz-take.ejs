            <div class="mdk-drawer-layout js-mdk-drawer-layout flex" data-fullbleed data-push data-has-scrolling-region>
                <div class="mdk-drawer-layout__content mdk-drawer-layout__content--scrollable">
                    <div class="container-fluid" id="body-main">

                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/dashboard">
                                    Home</a></li>
                            <li class="breadcrumb-item active">Quiz</li>
                        </ol>
                        <div class="media-body media-middle">
                            <h3 class="mb-0" style="text-align: center;font-weight: bolder;color: blue;">
                                <%=quiz.title%>
                            </h3>
                        </div>
                        <% for(var i=0;i<quiz.questions.length;i++){ %>
                            <!-- 1 cau hoi -->
                            <form id=question<%=i%>>
                                <div class="card">
                                    <div id="<%=i%>" class="card-header bg-white">
                                        <div class="media align-items-center">
                                            <div class="media-left">
                                                <h4 class="mb-0"><strong>Question <%=(i+1)%>:</strong></h4>
                                            </div>
                                            <div class="media-body">
                                                <h4 class="card-title">
                                                    <%- quiz.questions[i].question%>
                                                </h4>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <%for(var z=0;z<quiz.questions[i].answers.length;z++){ %>
                                            <!-- 1 answer -->
                                            <div class="form-group">
                                                <label class="custom-control custom-radio mb-0">
                                                    <input type="radio" class="custom-control-input" type="radio"
                                                        name=ques<%=i%> id=ans<%=z%>
                                                        value=<%=(z)%>>
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">
                                                                <%- quiz.questions[i].answers[z]%>
                                                            </span>
                                                </label>
                                            </div>

                                            <%}%>
                                    </div>
                                </div>
                            </form>
                            <%}%>
                                <!-- het 1 cau hoi -->

                                <div class="card-footer">
                                    <a class="btn btn-success float-right" id="submit">
                                        Submit <i class="material-icons btn__icon--right">
                                            send</i></a>
                                </div>
                    </div>
                </div>
                <!-- Sidebar -->
                <div class="mdk-drawer js-mdk-drawer" data-align="end">
                    <div class="mdk-drawer__content">
                        <div class="sidebar sidebar-right sidebar-light bg-white o-hidden">
                            <div class="sidebar-p-y" data-simplebar data-simplebar-force-enabled="true">
                                <div class="sidebar-heading">Question</div>
                                <ul class="list-group list-group-fit mb-0">
                                    <% for(var i=0;i<quiz.questions.length;i++){ %>
                                        <!-- question tag -->
                                        <a href='#<%=i%>' class="list-group-item list-group-item-action card">
                                            <span class="media align-items-center">
                                                <div class="media-left">
                                                    <span class="fa-stack fa-1x">

                                                        <i class="fa fa-circle fa-stack-2x"
                                                            style="color:rgb(5, 5, 5)"></i>
                                                        <strong class="fa-stack-1x" style="color:white">
                                                            <%=(i+1)%>
                                                        </strong>
                                                    </span>

                                                </div>
                                                <span class="media-body media-middle">
                                                    Question <%=(i+1)%>
                                                </span>
                                            </span>
                                        </a>
                                        <!-- end question tag -->
                                        <%}%>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- // END Sidebar -->
            </div>




            <div class="mdk-drawer js-mdk-drawer" id="default-drawer">
                <div class="mdk-drawer__content ls-top-navbar-xs-up">
                    <div class="sidebar sidebar-left sidebar-light bg-white o-hidden">
                        <div class="sidebar-p-y" data-simplebar data-simplebar-force-enabled="true">

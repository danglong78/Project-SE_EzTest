        <!-- Init -->
        <script src="/js/countdown.js"></script>

        <script>

            var ans = [];
            $(document).ready(function () {
                $('#submit').click(function () {
                    var n = <%=quiz.questions.length%>;

                    for (var i = 0; i < n; i++) {
                        var temp = $('input[name=ques' + i + ']:checked', '#question' + i).val();
                        if (temp == undefined) {
                            ans.push(-1);
                        } else {
                            ans.push($('input[name=ques' + i + ']:checked', '#question' + i).val());
                        }
                    }
                    var abc = "<%= quiz._id %>";

                    var rs = {
                        ans_list: ans,
                        quiz_id: abc
                    }

                    var $form = $("<form />");
                    $form.attr("action", "http://localhost:3000/quiz_result");
                    $form.attr("method", "POST");
                    $form.append('<input type="hidden" name="ans_list" value="' + rs.ans_list + '" />');
                    $form.append('<input type="hidden" name="quiz_id" value="' + rs.quiz_id + '" />');
                    $("body").append($form);
                    $form.submit();
                    console.log("done");
                });
            });
        </script>



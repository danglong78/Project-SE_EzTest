                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">Result</h4>
                        </div>
                        <div class="card-body media">
                            <div class="media-body media-middle">
                                <h4 class="mb-0">5.0</h4>
                                <span class="text-muted-light">Good</span>
                            </div>
                            <div class="media-right media-middle">
                                <a href="take-quiz.html" class="btn btn-primary">Restart <i
                                        class="material-icons btn__icon--right">refresh</i></a>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">Questions</h4>
                        </div>
                        <ul class="list-group list-group-fit mb-0">
                            <%for(var i=0;i<quiz.questions.length;i++){%>
                                <!-- 1 cau hoi -->
                                <li class="list-group-item">
                                    <div class="card">
                                        <div id="9" class="card-header bg-white">
                                            <div class="media align-items-center">
                                                <div class="media-left">
                                                    <h4 class="mb-0"><strong>Câu <%=(i+1)%></strong></h4>
                                                </div>
                                                <div class="media-body">
                                                    <h4 class="card-title">
                                                        <%=quiz.questions[i].question%>
                                                    </h4>
                                                </div>
                                                <div class="media-right">
                                                    <%if(quiz.questions[i].right_answer===ans_list[i]){%>
                                                        <span class="badge badge-success ">Correct</span>
                                                        <%}else{%>
                                                            <span class="badge badge-danger ">Wrong</span>
                                                            <%}%>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <%for(var z=0;z<quiz.questions[i].answers.length;z++){%>
                                            <!-- 1 cau tra loi -->
                                            <%if((z+1)==quiz.questions[i].right_answer && (z+1)==ans_list[i]){%>
                                                <div class="form-group">
                                                    <label class="custom-control custom-checkbox mb-0"
                                                        style="color: green;">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="customCheckBox" checked onclick="return false;" />
                                                        <span class="custom-control-indicator"></span>
                                                        <span class="custom-control-description"></span>
                                                        <%=quiz.questions[i].answers[z]%>
                                                            </span>
                                                    </label>
                                                </div>
                                                <%}%>
                                                    <%if((z+1)==quiz.questions[i].right_answer && (z+1)!=ans_list[i]){%>
                                                        <div class="form-group">
                                                            <label class="custom-control custom-checkbox mb-0"
                                                                style="color: green;">
                                                                <input type="checkbox" class="custom-control-input"
                                                                    id="customCheckBox" onclick="return false;" />
                                                                <span class="custom-control-indicator"></span>
                                                                <span class="custom-control-description">
                                                                    <%=quiz.questions[i].answers[z]%>
                                                                </span>
                                                            </label>
                                                        </div>
                                                        <%}%>
                                                            <%if((z+1)!=quiz.questions[i].right_answer &&
                                                                (z+1)==ans_list[i]){%>
                                                                <div class="form-group">
                                                                    <label class="custom-control custom-checkbox mb-0"
                                                                        style="color:red;">
                                                                        <input type="checkbox"
                                                                            class="custom-control-input"
                                                                            id="customCheckBox" checked
                                                                            onclick="return false;" />
                                                                        <span class="custom-control-indicator"></span>
                                                                        <span class="custom-control-description">
                                                                            <%=quiz.questions[i].answers[z]%>
                                                                        </span>
                                                                    </label>
                                                                </div>
                                                                <%}%>
                                                                    <%if((z+1)!=quiz.questions[i].right_answer &&
                                                                        (z+1)!=ans_list[i]){%>
                                                                        <div class="form-group">
                                                                            <label
                                                                                class="custom-control custom-checkbox mb-0">
                                                                                <input type="checkbox"
                                                                                    class="custom-control-input"
                                                                                    id="customCheckBox"
                                                                                    onclick="return false;" />
                                                                                <span
                                                                                    class="custom-control-indicator"></span>
                                                                                <span
                                                                                    class="custom-control-description">
                                                                                    <%=quiz.questions[i].answers[z]%>
                                                                                </span>
                                                                            </label>
                                                                        </div>
                                                                        <%}%>

                                                                            <!-- Het 1 cau tra loi -->
                                                                            <%}%>
                                    </div>
                                </li>
                                <!-- Het 1 cau hoi -->
                                <%}%>
                        </ul>
                    </div>

